{
  "name": "ferow2k/yii2-export",
  "description": "Just another yii2 export menu component.",
  "keywords": ["yii", "extension", "export"],
  "homepage": "https://github.com/ericmaicon/yii2-export",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Eric Maicon",
      "email": "eric@ericmaicon.com.br"
    },
    {
      "name": "Fernando Wentland",
      "email": "ferow2k@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "openspout/openspout": "^3.7",
    "yiisoft/yii2": "2.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Da\\export\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
